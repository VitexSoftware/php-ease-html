{
    "name": "vitexsoftware/ease-html",
    "description": "Set of HTML rendering objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Vitex Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "vitexsoftware/ease-core": ">= 1.41"
    },
    "autoload": {
        "psr-4": {
            "Ease\\": "src/Ease/",
            "Ease\\Html\\": "src/Ease/Html/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": ["tests/src/Ease/", "vendor/vitexsoftware/ease-core/tests/src/Ease/"],
            "Test\\Ease\\Html\\": "tests/src/Ease/Html/",
            "Test\\": "testing/",
            "Example\\Ease\\Html\\": "Examples"
        }
    },
    "require-dev": {
        "phpunit/phpunit":"^10.0",
        "phpstan/phpstan":"^1.10"
    }
}
