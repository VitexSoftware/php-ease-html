{
    "name": "vitexsoftware/ease-html",
    "description": "Set of HTML rendering objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Vitex Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "php": ">=8.1",
        "vitexsoftware/ease-core": "^1.48"
    },
    "autoload": {
        "psr-4": {
            "Ease\\": "src/Ease/",
            "Ease\\Html\\": "src/Ease/Html/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": ["tests/src/Ease/", "vendor/vitexsoftware/ease-core/tests/src/Ease/"],
            "Test\\Ease\\Html\\": "tests/src/Ease/Html/",
            "Test\\": "testing/",
            "Example\\Ease\\Html\\": "Examples"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.85",
        "ergebnis/php-cs-fixer-config": "^6.52",
        "ergebnis/composer-normalize": "^2.47"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
